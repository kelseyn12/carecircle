  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.9.0",
    "expo-server-sdk": "^3.7.0"
  },
  "devDependencies": {
    "typescript": "^4.9.0"
  },
  "private": true
}
